export LD_LIBRARY_PATH=./:$LD_LIBRARY_PATH:/root/anaconda3/envs/registration/lib/
export PYTHONPATH=$PYTHONPATH:/home/runtime/wrapper:/root/anaconda3/envs/registration/lib/python3.7/site-packages/