conda create -n registration python=3.7
conda activate registration
codna install scikit-image
conda install matplotlib
conda install -c onda-forge opencv
conda install cython
conda install gcc_linux-64
conda install numpy
conda list

export AIGES_WRAPPER_VERSION=1.0.0
